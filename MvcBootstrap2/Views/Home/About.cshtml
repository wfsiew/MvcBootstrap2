@model IEnumerable<MvcBootstrap2.ViewModels.EnrollmentDateGroup>

@using MvcBootstrap2.ViewModels;

@{
    ViewBag.Title = "Student Body Statistics";
}

<h2>Student Body Statistics</h2>

<table class="table table-striped table-hover table-condensed">
    <thead>
        <tr class="well">
            <th>
                Enrollment Date
            </th>
            <th>
                Students
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (EnrollmentDateGroup item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.EnrollmentDate)
                </td>
                <td>
                    @item.StudentCount
                </td>
            </tr>
        }
    </tbody>
</table>
